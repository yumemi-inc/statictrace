{
  "name": "statictrace",
  "version": "1.0.0-beta.2",
  "description": "A library for semi-automatic static testing.",
  "keywords": [
    "testing",
    "static analysis",
    "tool",
    "parsers",
    "compilers"
  ],
  "homepage": "https://github.com/jlkiri/statictrace#readme",
  "bugs": {
    "url": "https://github.com/jlkiri/statictrace/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jlkiri/statictrace.git"
  },
  "license": "MIT",
  "contributors": [
    "Kirill Vasiltsov",
    "Nobuyuki Tsutsui"
  ],
  "main": "src/lib.ts",
  "scripts": {
    "test": "jest",
    "test:update": "jest --updateSnapshot",
    "watch": "tsc -w",
    "parse": "node build/lib.js",
    "build": "tsc",
    "format": "prettier -w ."
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "commander": "^7.2.0",
    "ts-morph": "^11.0.0",
    "typescript": "~4.3.2"
  },
  "devDependencies": {
    "dotenv": "^10.0.0",
    "@types/node": "^15.6.1",
    "jest": "^27.0.4",
    "prettier": "^2.3.1"
  },
  "files": [
    "build"
  ]
}
